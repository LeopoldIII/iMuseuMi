/* pages/FMSearchAnalysisBound/index.wxss */
.container {
    height: 100%;
    overflow: hidden;
}

.boundSearch{
    display: flex;
    position: absolute;
    left: 5%;
    top: 20rpx;
    /* width: 60%; */
    height: 64rpx;
    line-height: 64rpx;
    z-index: 1;
}

.boundSearch input{
    width: 120rpx;
    height: 100%;
    padding: 0 5px;
    border-radius: 3px;
    box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.075);
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border: 1px solid #ccc;
    background: #fff;
    margin-right: 10rpx;
}

.boundSearch button{
    margin-left: 10rpx;
}