/* pages/FMNavigationBase/index.wxss */
.container {
    height: 100%;
    overflow: hidden;
}

.tip {
    position: absolute;
    bottom: 80rpx;
    left: 20rpx;
    right: 20rpx;
    font-size: 12px;
    line-height: 64rpx;
    background: rgba(204, 204, 204, 0.26);
    padding: 0 20rpx;
    z-index: 1;
}

.boundSearch{
    position: absolute;
    left: 5%;
    top: 20rpx;
    /* width: 60%; */
    height: 64rpx;
    line-height: 64rpx;
    z-index: 0;
    top: 620rpx;
    display:flex;                    
    justify-content: center;         
    align-items:center;    
  }

.boundSearch input{
    width: 120rpx;
    height: 100%;
    padding: 0 5px;
    border-radius: 3px;
    box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.075);
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border: 1px solid #ccc;
    background: #fff;
    margin-right: 10rpx;
}

.boundSearch button{
    margin-left: 10rpx;
}

